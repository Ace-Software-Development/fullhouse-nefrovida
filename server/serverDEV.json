{
    "apps": [{
        "name": "ParseDevServerNefroWeb",
        "script": "app.js",
        "watch": ["cloud", "functions", "app.js", "controllers", "models"],
        "watch_options": {
            "useFsEvents": false
        },
        "ignore_watch": ["public/", "node_modules/"],
        "env": {
            "NAME": "NefroWebServerDEV",
            "APP_NAME": "NefroWeb Server DEV",
            "PORT": 6535,
            "NODE_ENV": "development",
            "DATABASE_URI": "mongodb://127.0.0.1:27017/nefrowebDEV",
            "APP_ID": "app_id",
            "MASTER_KEY": "master_key",
            "SECRET_KEY_USERS": "secret_key",
            "SERVER_URL": "http://localhost:6535/parse"
        }
    }]
}